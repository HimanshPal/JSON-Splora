{
  "name": "JSON-Splora",
  "version": "0.0.3",
  "description": "GUI for editing, visualizing, and manipulating JSON data",
  "main": "index.js",
  "bin": {
    "jsplora": "bin/jsplora"
  },
  "scripts": {
    "start": "ENV=development electron .",
    "build-darwin": "node_modules/.bin/electron-packager . --darwin --overwrite",
    "build-linux": "node_modules/.bin/electron-packager . --linux --overwrite",
    "build-win32": "node_modules/.bin/electron-packager . --win32 --overwrite"
  },
  "keywords": [
    "Electron",
    "CodeMirror",
    "jq",
    "json",
    "json5"
  ],
  "author": "wellsjo",
  "devDependencies": {
    "electron-packager": "^8.3.0"
  },
  "dependencies": {
    "electron": "^1.4.8",
    "codemirror": "^5.20.2",
    "electron-context-menu": "^0.8.0",
    "electron-default-menu": "^1.0.0",
    "jquery": "^3.1.1",
    "js-beautify": "^1.6.4",
    "json5": "^0.5.0",
    "jsonlint": "^1.6.2",
    "menu": "^0.2.5",
    "node-jq": "^0.4.0"
  }
}
